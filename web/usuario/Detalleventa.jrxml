	<queryString>
		<![CDATA[select distinct p.pedido as 'pedido',totalprod,dp.estilo,dp.corrida,c.descripcion,
c15 = case when dp.Corrida =90 THEN SUM(cant1+cant2) else '' END,
c16 = case when dp.corrida=90 then SUM(cant3+cant4) else '' END,
c17 = case when dp.corrida=90 then sum(cant5+cant6) when dp.corrida=97 then SUM(cant1+cant2) else '' END,
c18 = case when dp.corrida=91 then sum(cant1+cant2) when dp.corrida=97 then SUM(cant3+cant4) else '' END,
c19 = case when dp.corrida=91 then sum(cant3+cant4) when dp.corrida=97 then SUM(cant5+cant6) else '' END,
c20 = case when dp.corrida=91 then sum(cant5+cant6) when dp.corrida=97 then SUM(cant7+cant8) else '' END,
c21 = case when dp.corrida=91 then sum(cant7+cant8) when dp.corrida=97 then SUM(cant9+cant10) when dp.corrida=95 then SUM(cant1+cant2) else '' END,
c22 = case when dp.corrida=92 then sum(cant1+cant2) when dp.corrida=93 then SUM(cant1+cant2) when dp.corrida=95 then SUM(cant3+cant4) else '' END,
c23 = case when dp.corrida=92 then sum(cant3+cant4) when dp.corrida=93 then SUM(cant3+cant4) when dp.corrida=95 then SUM(cant5+cant6) when dp.corrida=98 then SUM(cant1+cant2) else '' END,
c24 = case when dp.corrida=92 then sum(cant5+cant6) when dp.corrida=93 then SUM(cant5+cant6) when dp.corrida=95 then SUM(cant7+cant8) when dp.corrida=98 then SUM(cant3+cant4) else '' END,
c25 = case when dp.corrida=93 then SUM(cant7+cant8) when dp.corrida=95 then SUM(cant9+cant10) when dp.corrida=98 then SUM(cant5+cant6) when dp.corrida=94 then SUM(cant1+cant2) else '' END,
c26 = case when dp.corrida=93 then SUM(cant9+cant10) when dp.corrida=94 then SUM(cant3+cant4) when dp.corrida=98 then SUM(cant7+cant8)else '' END,
c27 = case when dp.corrida=94 then SUM(cant5+cant6) when dp.corrida=98 then SUM(cant9+cant10) else '' END,
c28 = case when dp.corrida=94 then SUM(cant7+cant8) else '' END,
c29 = case when dp.corrida=94 then SUM(cant9+cant10) else '' END,
c30 = case when dp.corrida=94 then SUM(cant11+cant12) when dp.corrida=96 then SUM(cant1+cant2) else '' END,
c31 = case when dp.corrida=96 then SUM(cant3+cant4) else '' END,
c32 = case when dp.corrida=96 then SUM(cant5+cant6) else '' END,
c33 = case when dp.corrida=96 then SUM(cant7+cant8) else '' END
from pedidos p join DPedidos dp on dp.clave_pedido=p.clave_pedido 
join Productos prod on dp.producto=prod.producto join Combinaciones c on dp.combinacion = c.combinacion where  p.pedido like '%$P!{pedido}%'  and convert(date,fechapedido) between $P{f1} and $P{f2} 
group by p.pedido,dp.corrida,dp.estilo,totalprod,c.descripcion
 order by pedido DESC]]>
	</queryString>
